{
	"preset": "ts-jest/presets/js-with-ts-esm",
	"moduleNameMapper": {
		"^(\\.{1,2}/.*)\\.js$": "$1"
	},
	"transform": {
		"^.+\\.tsx?$": [
			"ts-jest",
			{
				"useESM": true
			}
		]
	},
	"transformIgnorePatterns": ["node_modules/(?!(@stenway/reliabletxt|@stenway/reliabletxt-io|@stenway/wsv|@stenway/wsv-io|@stenway/sml|@stenway/sml-io|@stenway/tbl))"]
}